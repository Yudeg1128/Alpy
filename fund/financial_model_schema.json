{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Debt Instrument Issuer Financial Model Schema",
  "description": "Schema for a comprehensive financial model (v2.3 - Mongolian names integrated, focus on better context gathering).",
  "type": "object",
  "$defs": {
    "period_object_value_only": {
      "type": "object",
      "properties": {
        "period_label": {
          "type": "string",
          "description": "Label for the period, e.g., FY2023, FY_Proj_1."
        },
        "value": {
          "type": ["number", "string", "null"],
          "description": "Value for the item in this period."
        },
        "source_reference": {
          "type": ["string", "null"],
          "description": "Reference to where the data was found."
        },
        "extraction_status": {
          "type": ["string", "null"],
          "description": "Status of data extraction for this period (e.g., SUCCESS, NOT_FOUND_PER_LLM, FAILURE_...)."
        }
      },
      "required": ["period_label"],
      "additionalProperties": true
    },
    "assumption_period_object": {
      "type": "object",
      "properties": {
        "period_label": { "type": "string" },
        "value": { "type": ["number", "null"] },
        "unit": { "type": "string" },
        "data_type": { "type": "string" },
        "rationale": { "type": ["string", "null"] },
        "ai_instructions": { "type": ["string", "null"] }
      },
      "required": ["period_label", "value", "unit", "data_type"],
      "additionalProperties": false
    },
    "financial_line_item_base_schema": {
      "type": "object",
      "properties": {
        "name": { "type": "string", "description": "Name of the financial line item (English)." },
        "name_mn": { "type": ["string", "null"], "description": "Name of the financial line item (Mongolian)." },
        "data_type": { "type": "string", "default": "currency_value", "description": "Semantic data type." },
        "periods": {
          "type": "array", "default": [],
          "items": { "$ref": "#/$defs/period_object_value_only" },
          "description": "Array of values for historical and projected periods."
        },
        "is_calculated": { "type": "boolean", "description": "True if calculated." },
        "calculation_logic_description": { "type": ["string", "null"] },
        "source_guidance_historical": { "type": ["string", "null"] },
        "ai_instructions": { "type": ["string", "null"] },
        "ai_instructions_projected": { "type": ["string", "null"] },
        "notes": { "type": ["string", "null"] },
        "unit": { "type": ["string", "null"] }
      },
      "required": ["name", "data_type", "periods", "is_calculated"],
      "additionalProperties": true
    },
    "debt_tranche_detail_schema": {
        "type": "object",
        "description": "Detailed information for a specific debt tranche.",
        "properties": {
            "tranche_name": {"type": "string"},
            "name_mn": {"type": ["string", "null"], "description": "Нэрийн бичиг баримтын нэр (Монгол)"},
            "original_principal": {"type": ["number", "null"]},
            "outstanding_principal_start_of_model": {"type": ["number", "null"]},
            "issue_date": {"type": ["string", "null"], "format": "date"},
            "maturity_date": {"type": ["string", "null"], "format": "date"},
            "currency": {"type": ["string", "null"]},
            "interest_rate_type": {"type": ["string", "null"], "enum": ["fixed", "floating", "zero_coupon", "pik"]},
            "interest_rate_value_or_formula": {"type": ["string", "null"]},
            "interest_payment_frequency": {"type": ["string", "null"], "enum": ["monthly", "quarterly", "semi-annually", "annually"]},
            "principal_repayment_terms": {"type": ["string", "null"]},
            "security_details": {"type": ["string", "null"]},
            "seniority": {"type": ["string", "null"]},
            "covenants": {"type": ["string", "null"]},
            "periodic_movements": {
                "type": "array", "default": [],
                "items": {
                    "type": "object",
                    "properties": {
                        "period_label": {"type": "string"},
                        "beginning_balance": {"type": ["number", "null"]},
                        "new_borrowings_drawdowns": {"type": ["number", "null"]},
                        "principal_repayments_scheduled": {"type": ["number", "null"]},
                        "principal_repayments_optional": {"type": ["number", "null"]},
                        "interest_expense_accrued": {"type": ["number", "null"]},
                        "pik_interest_accrued": {"type": ["number", "null"]},
                        "cash_interest_paid": {"type": ["number", "null"]},
                        "fees_paid": {"type": ["number", "null"]},
                        "ending_balance": {"type": ["number", "null"]}
                    },
                    "required": ["period_label"]
                }
            }
        },
        "required": ["tranche_name"],
        "additionalProperties": false
    },
    "wacc_input_item_schema": {
        "type": "object",
        "properties": {
            "name": {"type": "string"},
            "name_mn": {"type": ["string", "null"]},
            "value": {"type": ["number", "null"]},
            "unit": {"type": "string"},
            "ai_instructions": {"type": ["string", "null"]},
            "source_guidance": {"type": ["string", "null"]},
            "is_calculated": {"type": "boolean", "default": false},
            "calculation_logic_description": {"type": ["string", "null"]}
        },
        "required": ["name", "unit"]
    },
    "simple_value_object_schema":{
        "type": "object",
        "properties": {
            "value": {"type": ["number", "string", "boolean", "null"]},
            "is_calculated": {"type": "boolean", "default": true},
            "calculation_logic_description": {"type": ["string", "null"]}
        },
        "required": ["is_calculated"] 
    },
    "assumption_override_item_schema": {
        "type": "object",
        "properties": {
            "assumption_path": {"type": "string", "description": "JSON path to the assumption being overridden."},
            "new_value_or_driver": {"type": "any", "description": "The overriding value or new driver logic."},
            "period_label_specific": {"type": ["string", "null"], "description": "If override is for a specific period."}
        },
        "required": ["assumption_path", "new_value_or_driver"]
    },
    "known_covenant_item_schema": {
        "type": "object",
        "properties": {
            "covenant_name": {"type": "string"},
            "name_mn": {"type": ["string", "null"]},
            "metric_definition": {"type": "string"},
            "threshold_condition": {"type": "string"},
            "compliance_status_projection": {
                "type": "array", "default": [],
                "items": {"$ref": "#/$defs/period_object_value_only"}
            }
        },
        "required": ["covenant_name", "metric_definition", "threshold_condition"]
    },
    "confidence_score_item_schema": {
        "type": "object",
        "properties": {
            "projection_item_name": {"type": "string"},
            "name_mn": {"type": ["string", "null"]},
            "confidence_score_percentage": {"type": ["number", "null"], "minimum": 0, "maximum": 100},
            "reasoning": {"type": ["string", "null"]}
        },
        "required": ["projection_item_name"]
    }
  },
  "properties": {
    "model_metadata": {
        "type": "object",
        "properties": {
          "model_name": { "type": "string", "default": "Debt Instrument Issuer Financial Model" },
          "version": { "type": "string", "default": "2.3_mongolian_names" },
          "target_company_name": { "type": ["string", "null"] },
          "ticker_symbol": { "type": ["string", "null"] },
          "currency": { "type": "string", "default": "MNT" },
          "fiscal_year_end": { "type": "string", "pattern": "^(0[1-9]|1[0-2])-(0[1-9]|[12][0-9]|3[01])$" },
          "historical_periods_count": { "type": "integer", "minimum": 0, "default": 2 },
          "projection_periods_count": { "type": "integer", "minimum": 0, "default": 3 },
          "analyst_name": { "type": "string", "default": "AI Financial Modeler Agent" },
          "date_created": { "type": "string", "format": "date-time" },
          "ai_instructions_general": { "type": "string", "default": "This model is for debt analysis. Pay attention to Mongolian financial reporting standards if context implies." },
          "model_focus": { "type": "string", "default": "Fixed Income Securities Analysis" }
        },
        "required": [
          "model_name", "version", "currency", "fiscal_year_end", "historical_periods_count",
          "projection_periods_count", "analyst_name", "date_created", "ai_instructions_general", "model_focus"
        ],
        "additionalProperties": false
    },
    "global_assumptions": {
        "type": "array", "default": [],
        "items": { "$ref": "#/$defs/assumption_period_object" }
    },
    "company_specific_assumptions_debt_focus": {
        "type": "object", "default": {},
        "properties": {
          "section_description": { "type": "string", "default": "Компанийн тусгай таамаглалууд (өрийн төвлөрөл)" }, 
          "ai_instructions_section": { "type": "string", "default": "Түүхэн мэдээлэл болон баримт бичгийн агуулгад үндэслэн таамаглал боловсруулна уу." },
          "revenue_drivers": { "type": "array", "default": [], "items": { "type": "object", "properties": { "segment_name": { "type": "string" }, "name_mn": { "type": ["string", "null"] }, "driver_type": { "type": "string" }, "periods": { "type": "array", "items": { "$ref": "#/$defs/assumption_period_object" } } }, "required": ["segment_name", "driver_type", "periods"]} },
          "cost_and_margin_drivers": { "type": "array", "default": [], "items": { "type": "object", "properties": { "cost_category": { "type": "string" }, "name_mn": { "type": ["string", "null"] }, "driver_type": { "type": "string" }, "periods": { "type": "array", "items": { "$ref": "#/$defs/assumption_period_object" } } }, "required": ["cost_category", "driver_type", "periods"]} },
          "working_capital_drivers": { "type": "array", "default": [], "items": { "type": "object", "properties": { "item_name": { "type": "string" }, "name_mn": { "type": ["string", "null"] }, "periods": { "type": "array", "items": { "$ref": "#/$defs/assumption_period_object" } } }, "required": ["item_name", "periods"]} },
          "capital_expenditure_and_asset_drivers": { "type": "array", "default": [], "items": { "type": "object", "properties": { "item_name": { "type": "string" }, "name_mn": { "type": ["string", "null"] }, "periods": { "type": "array", "items": { "$ref": "#/$defs/assumption_period_object" } } }, "required": ["item_name", "periods"]} },
          "financing_and_debt_assumptions": { "type": "object", "default": {}, "properties": { "existing_debt_refinancing_strategy": { "type": ["string", "null"] }, "new_debt_issuance_plans": { "type": "array", "default": [], "items": { "$ref": "#/$defs/debt_tranche_detail_schema" }}, "assumed_interest_rate_on_new_debt": { "type": ["number", "null"] }, "dividend_policy_and_payout_ratio": { "type": "array", "default": [], "items": { "$ref": "#/$defs/assumption_period_object" } }, "share_repurchases_policy": { "type": "array", "default": [], "items": { "$ref": "#/$defs/assumption_period_object" } }, "debt_paydown_priority": { "type": ["string", "null"] }, "cash_sweep_mechanism_for_excess_cash_flow": { "type": ["string", "null"] }, "minimum_cash_balance_target": { "type": ["number", "null"] } } },
          "tax_assumptions": { "type": "object", "default": {}, "properties": { "effective_tax_rate": { "type": "array", "default": [], "items": { "$ref": "#/$defs/assumption_period_object" } }, "cash_tax_rate_if_different": { "type": "array", "default": [], "items": { "$ref": "#/$defs/assumption_period_object" } } } }
        },
        "additionalProperties": false
    },
    "financial_statements_core": {
      "type": "object", "default": {},
      "properties": {
        "section_description": { "type": "string", "default": "Үндсэн санхүүгийн тайлангууд." },
        "historical_period_labels": { "type": "array", "items": { "type": "string" }, "default": [] },
        "projected_period_labels": { "type": "array", "items": { "type": "string" }, "default": [] },
        "income_statement": {
          "type": "object", "default": {},
          "properties": {
            "ai_instructions_statement": { "type": "string", "default": "Орлогын тайлангийн зүйлсийг гаргаж, төсөөлөл боловсруулна уу. Монгол тайланд COGS, SG&A, EBITDA, EBIT, EBT зэрэг англи товчлол ашиглагдаж болзошгүйг анхаарна уу." },
            "line_items": {
              "type": "array", "default": [],
              "items": [
                { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Revenue"}, "name_mn": {"const": "Орлого"}, "is_calculated": {"const": false}, "source_guidance_historical": {"default": "Reported Net Revenue from 10-K/Q. / Тайлант цэвэр орлого."}} },
                { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Cost of Goods Sold (COGS)"}, "name_mn": {"const": "Борлуулсан барааны өртөг"}, "is_calculated": {"const": false}, "source_guidance_historical": {"default": "Reported COGS. / Тайлант ББӨ."}} },
                { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Gross Profit"}, "name_mn": {"const": "Нийт ашиг"}, "is_calculated": {"const": true}, "calculation_logic_description": {"default": "Revenue - COGS / Орлого - ББӨ"}} },
                { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "SG&A Expenses"}, "name_mn": {"const": "Худалдан борлуулалт, ерөнхий ба захиргааны зардал"}, "is_calculated": {"const": false}, "source_guidance_historical": {"default": "Reported SG&A."}} },
                { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Other Operating Expenses/Income"}, "name_mn": {"const": "Бусад үйл ажиллагааны орлого, зардал"}, "is_calculated": {"const": false}, "source_guidance_historical": {"default": "Reported. Normalize."}} },
                { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "EBITDA (Earnings Before Interest, Taxes, Depreciation & Amortization)"}, "name_mn": {"const": "Хүү, татвар, элэгдэл, хорогдлын өмнөх ашиг"}, "is_calculated": {"const": true}, "calculation_logic_description": {"default": "Gross Profit - SG&A - Other OpEx (excl. D&A). EBITDA abbreviation may be used."}} },
                { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Depreciation & Amortization (D&A)"}, "name_mn": {"const": "Элэгдэл ба хорогдол"}, "is_calculated": {"const": false}, "source_guidance_historical": {"default": "From CFS or BS roll-forward."}} },
                { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "EBIT (Earnings Before Interest & Taxes)"}, "name_mn": {"const": "Хүү, татварын өмнөх ашиг"}, "is_calculated": {"const": true}, "calculation_logic_description": {"default": "EBITDA - D&A. EBIT abbreviation may be used."}} },
                { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Interest Expense"}, "name_mn": {"const": "Хүүгийн зардал"}, "is_calculated": {"const": false}, "source_guidance_historical": {"default": "Reported Interest Expense."}} },
                { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Interest Income"}, "name_mn": {"const": "Хүүгийн орлого"}, "is_calculated": {"const": false}, "source_guidance_historical": {"default": "Reported Interest Income."}} },
                { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "EBT (Earnings Before Tax)"}, "name_mn": {"const": "Татварын өмнөх ашиг"}, "is_calculated": {"const": true}, "calculation_logic_description": {"default": "EBIT - Interest Expense + Interest Income. EBT abbreviation may be used."}} },
                { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Income Tax Expense"}, "name_mn": {"const": "Орлогын татварын зардал"}, "is_calculated": {"const": false}, "source_guidance_historical": {"default": "Reported Tax Expense."}} },
                { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Net Income"}, "name_mn": {"const": "Цэвэр ашиг"}, "is_calculated": {"const": true}, "calculation_logic_description": {"default": "EBT - Income Tax Expense"}} }
              ]
            }
          }, "required": ["line_items"], "additionalProperties": false
        },
        "balance_sheet": {
          "type": "object", "default": {},
          "properties": {
            "ai_instructions_statement": { "type": "string", "default": "Хөрөнгө = Өр төлбөр + Эздийн өмч гэдгийг баталгаажуулна уу. Монгол тайланд PP&E зэрэг англи товчлол ашиглагдаж болзошгүй." },
            "assets": {
              "type": "object", "default": {},
              "properties": {
                "current_assets": { "type": "array", "default": [], "items": [
                  { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Cash & Cash Equivalents"}, "name_mn": {"const": "Мөнгөн хөрөнгө ба түүнтэй адилтгах хөрөнгө"}, "is_calculated": {"const": false}} },
                  { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Accounts Receivable"}, "name_mn": {"const": "Авлагын данс"}, "is_calculated": {"const": false}} },
                  { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Inventory"}, "name_mn": {"const": "Бараа материал"}, "is_calculated": {"const": false}} }
                ]},
                "non_current_assets": { "type": "array", "default": [], "items": [
                  { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Net PP&E"}, "name_mn": {"const": "Үндсэн хөрөнгө, цэвэр дүнгээр"}, "is_calculated": {"const": false}, "source_guidance_historical": {"default": "Net Property, Plant & Equipment. PP&E abbreviation may be used."}} },
                  { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Goodwill"}, "name_mn": {"const": "Гүүдвил"}, "is_calculated": {"const": false}} },
                  { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Intangible Assets (Net)"}, "name_mn": {"const": "Биет бус хөрөнгө, цэвэр дүнгээр"}, "is_calculated": {"const": false}} }
                ]},
                "total_assets": { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Total Assets"}, "name_mn": {"const": "Нийт хөрөнгө"}, "is_calculated": {"const": true}} }
              }, "required": ["current_assets", "non_current_assets", "total_assets"], "additionalProperties": false
            },
            "liabilities_and_equity": {
              "type": "object", "default": {},
              "properties": {
                "current_liabilities": { "type": "array", "default": [], "items": [
                  { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Accounts Payable"}, "name_mn": {"const": "Өглөгийн данс"}, "is_calculated": {"const": false}} },
                  { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Short-Term Debt & Current Portion of LTD"}, "name_mn": {"const": "Богино хугацаат өр төлбөр ба УХӨТ-ийн эргэн төлөгдөх хэсэг"}, "is_calculated": {"const": false}} }
                ]},
                "non_current_liabilities": { "type": "array", "default": [], "items": [
                  { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Long-Term Debt"}, "name_mn": {"const": "Урт хугацаат өр төлбөр"}, "is_calculated": {"const": false}} }
                ]},
                "total_liabilities": { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Total Liabilities"}, "name_mn": {"const": "Нийт өр төлбөр"}, "is_calculated": {"const": true}} },
                "equity": { "type": "array", "default": [], "items": [
                  { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Common Stock & APIC"}, "name_mn": {"const": "Энгийн хувьцаа ба Нэмж төлөгдсөн капитал"}, "is_calculated": {"const": false}} },
                  { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Retained Earnings"}, "name_mn": {"const": "Хуримтлагдсан ашиг"}, "is_calculated": {"const": false}} }
                ]},
                "total_equity": { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Total Equity"}, "name_mn": {"const": "Нийт эзэмшигчдийн өмч"}, "is_calculated": {"const": true}} },
                "total_liabilities_and_equity": { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Total Liabilities & Equity"}, "name_mn": {"const": "Нийт өр төлбөр ба эзэмшигчдийн өмч"}, "is_calculated": {"const": true}} }
              }, "required": ["current_liabilities", "non_current_liabilities", "total_liabilities", "equity", "total_equity", "total_liabilities_and_equity"], "additionalProperties": false
            },
            "balance_sheet_check": { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Balance Sheet Check (Assets - L&E)"}, "name_mn": {"const": "Тэнцлийн шалгалт (Хөрөнгө - ӨТ ба ЭӨ)"}, "is_calculated": {"const": true}, "target_value": {"type":"number", "const": 0}, "tolerance": {"type":"number", "default": 1}} }
          }, "required": ["assets", "liabilities_and_equity", "balance_sheet_check"], "additionalProperties": false
        },
        "cash_flow_statement": {
          "type": "object", "default": {},
          "properties": {
            "ai_instructions_statement": { "type": "string", "default": "Орлогын тайлан ба тайлан балансын өөрчлөлтөөс гаргана. Монгол тайланд Capex зэрэг англи товчлол ашиглагдаж болзошгүй." },
            "cash_flow_from_operations_cfo": { "type": "array", "default": [], "items": [
              { "allOf": [{"$ref": "#/$defs/financial_line_item_base_schema"}], "properties": {"name": {"const": "Net Income"}, "name_mn": {"const": "Цэвэр ашиг"}, "is_calculated": {"const": false} } },
